{
  "version": 2,
  "routes": [
    {
      "src": "/api/videos",
      "dest": "/api/videos.js"
    },
    {
      "src": "/api/download/(.*)",
      "dest": "/api/download.js"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/videos/$1"
    },
    {
      "src": "/(.*\\.(css|js|html|ico|png|jpg|jpeg|gif|svg))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/videos.js": {
      "maxDuration": 10
    },
    "api/download.js": {
      "maxDuration": 30
    }
  }
}